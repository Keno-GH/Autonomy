            <li>DoctorTendToAnimals</li>
        </targetWorkGivers>

    </Autonomy.PriorityGiverDef>

    <!-- Infogiver for average of tend quality, individualizable -->
    <!-- We use tendquality because it changes with more than just skill -->
    <!-- If our doctor is injured, high or in any other capacity incapacitated to do a proper -->
    <!-- job, checking with tend quality will capture that -->
	<Autonomy.InfoGiverDef>
		<defName>TendQualityAverage</defName>
		<label>Tend quality average</label>
		<description>Tracks each pawn's tend quality individually for personalized tend priority</description>
		<isLocalizable>false</isLocalizable>
		<isIndividualizable>true</isIndividualizable>
		<calculation>Avg</calculation>
		<isUrgent>false</isUrgent>
		<sourceType>pawnStat</sourceType>
		<targetStat>MedicalTendQuality</targetStat>
        
		<filters>
			<include>
				<li>player</li>
			</include>
			<exclude>
				<li>dead</li>
				<li>downed</li>
			</exclude>
		</filters>
	</Autonomy.InfoGiverDef>

