<Defs>

    <!-- Prioritygiver for CleanFilth using the FilthCount infogiver -->

    <Autonomy.PriorityGiverDef>
        <defName>CleanFilthCount</defName>
        <label>Cleaning by filth count</label>
        <description>Boosts cleaning urgency when home area filth accumulates.</description>
        <isUrgent>false</isUrgent>

        <conditions>
            <li>
                <type>infoGiver</type>
                <infoDefName>FilthCount</infoDefName>
                <requestLocalizedData>false</requestLocalizedData>
                <requestIndividualData>false</requestIndividualData>
                <requestDistanceFromGlobal>false</requestDistanceFromGlobal>
            </li>
            <li>
                <type>personalityOffset</type>
                <personalityDefName>Rimpsyche_Organization</personalityDefName>
                <personalityMultipliers>
                    <li>
                        <personalityRange>-1.0~-0.5</personalityRange>
                        <multiplier>0.75</multiplier>
                        <flat>-2</flat>
                    </li>
                    <li>
                        <personalityRange>-0.49~0.0</personalityRange>
                        <multiplier>0.9</multiplier>
                    </li>
                    <li>
                        <personalityRange>0.01~0.5</personalityRange>
                        <multiplier>1.1</multiplier>
                        <flat>2</flat>
                    </li>
                    <li>
                        <personalityRange>0.51~1.0</personalityRange>
                        <multiplier>1.25</multiplier>
                        <flat>4</flat>
                    </li>
                </personalityMultipliers>
            </li>
            <li>
                <type>personalityOffset</type>
                <personalityDefName>Rimpsyche_Diligence</personalityDefName>
                <personalityMultipliers>
                    <li>
                        <personalityRange>-1.0~-0.5</personalityRange>
                        <multiplier>0.8</multiplier>
                        <flat>-1</flat>
                    </li>
                    <li>
                        <personalityRange>-0.49~0.0</personalityRange>
                        <multiplier>0.9</multiplier>
                    </li>
                    <li>
                        <personalityRange>0.01~0.5</personalityRange>
                        <multiplier>1.1</multiplier>
                    </li>
                    <li>
                        <personalityRange>0.51~1.0</personalityRange>
                        <multiplier>1.2</multiplier>
                        <flat>1</flat>
                    </li>
                </personalityMultipliers>
            </li>
            <li>
                <type>personalityOffset</type>
                <personalityDefName>Rimpsyche_Expectation</personalityDefName>
                <personalityMultipliers>
                    <li>
                        <personalityRange>-1.0~-0.5</personalityRange>
                        <multiplier>0.85</multiplier>
                        <flat>-1</flat>
                    </li>
                    <li>
                        <personalityRange>-0.49~0.0</personalityRange>
                        <multiplier>0.95</multiplier>
                    </li>
                    <li>
                        <personalityRange>0.01~0.5</personalityRange>
                        <multiplier>1.05</multiplier>
                    </li>
                    <li>
                        <personalityRange>0.51~1.0</personalityRange>
                        <multiplier>1.2</multiplier>
                        <flat>1</flat>
                    </li>
                </personalityMultipliers>
            </li>
        </conditions>

        <priorityRanges>
            <li>
                <validRange>0~10</validRange>
                <priority>0</priority>
                <description>Things are clean, for now</description>
            </li>
            <li>
                <validRange>10~40</validRange>
                <priority>1~5</priority>
                <description>There is a bit of filth</description>
            </li>
            <li>
                <validRange>40~100</validRange>
                <priority>5~10</priority>
                <description>There is a lot of filth</description>
            </li>
            <li>
                <validRange>101~200</validRange>
                <priority>10~15</priority>
                <description>Everything is a bit dirty</description>
            </li>
            <li>
                <validRange>201~299</validRange>
                <priority>15~19</priority>
                <description>Everything is really dirty</description>
            </li>
            <li>
                <validRange>300~999</validRange>
                <priority>20</priority>
                <description>Everything is absolutely filthy</description>
            </li>
        </priorityRanges>

        <targetWorkGivers>
            <li>CleanFilth</li>
        </targetWorkGivers>

    </Autonomy.PriorityGiverDef>
    
