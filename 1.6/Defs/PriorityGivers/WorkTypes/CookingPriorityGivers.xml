<Defs>

    <!-- Cooking defName: DoBillsCook and DoBillsCookCampfire
    Butchering defName: DoBillsButcherFlesh
    Brewing defNames: DoBillsBrew and CookFillHopper-->

    <!-- Cooking, Butchering priority based on individual food pawnNeed InfoGiver-->

    <Autonomy.InfoGiverDef>
        <defName>FoodPoisonChance</defName>
        <label>Food poison chance</label>
        <description>Tracks each pawn's chance to avoid poisoning meals, letting skilled cooks step up.</description>
        <isLocalizable>false</isLocalizable>
        <isIndividualizable>true</isIndividualizable>
        <calculation>Avg</calculation>
        <isUrgent>false</isUrgent>
        <sourceType>pawnStat</sourceType>
        <targetStat>FoodPoisonChance</targetStat>

        <filters>
            <include>
                <li>player</li>
            </include>
            <exclude>
                <li>dead</li>
                <li>downed</li>
            </exclude>
        </filters>
    </Autonomy.InfoGiverDef>

    <Autonomy.InfoGiverDef>
        <defName>ButcheryFleshEfficiency</defName>
        <label>Butchery efficiency</label>
        <description>Tracks each pawn's butchery flesh efficiency to decide who should work the tables.</description>
        <isLocalizable>false</isLocalizable>
        <isIndividualizable>true</isIndividualizable>
        <calculation>Avg</calculation>
        <isUrgent>false</isUrgent>
        <sourceType>pawnStat</sourceType>
        <targetStat>ButcheryFleshEfficiency</targetStat>

        <filters>
            <include>
                <li>player</li>
            </include>
            <exclude>
                <li>dead</li>
                <li>downed</li>
            </exclude>
        </filters>
    </Autonomy.InfoGiverDef>

    <!-- Butchering priority based on flat ButcheryFleshEfficiency pawnStat InfoGiver
    (Represents butchering skill)
    (Perception of self vs expectations)-->

    <!-- Brewing priority based on flat DrugCookingSpeed pawnStat InfoGiver
    (Represents brewing skill)
    (Perception of self vs expectations)-->

    <Autonomy.InfoGiverDef>
        <defName>DrugCookingSpeed</defName>
        <label>Drug cooking speed</label>
        <description>Tracks each pawn's brewing speed for beer, tea, and simple drugs.</description>
        <isLocalizable>false</isLocalizable>
        <isIndividualizable>true</isIndividualizable>
        <calculation>Avg</calculation>
        <isUrgent>false</isUrgent>
        <sourceType>pawnStat</sourceType>
        <targetStat>DrugCookingSpeed</targetStat>

        <filters>
            <include>
                <li>player</li>
            </include>
            <exclude>
                <li>dead</li>
                <li>downed</li>
            </exclude>
        </filters>
    </Autonomy.InfoGiverDef>

    <!-- Priority Givers -->

    <!-- Cooking priority: hungry pawn takes action -->
    <Autonomy.PriorityGiverDef>
        <defName>CookPersonalHunger</defName>
        <label>Cook by personal hunger</label>
        <description>Prioritizes cooking based on the pawn's personal hunger level.</description>
        <isUrgent>true</isUrgent>

        <conditions>
            <li>
                <type>infoGiver</type>
                <infoDefName>ColonistFoodNeed</infoDefName>
                <requestLocalizedData>false</requestLocalizedData>
                <requestIndividualData>true</requestIndividualData>
                <requestNormalizedDistance>false</requestNormalizedDistance>
            </li>
            <li>
                <type>personalityOffset</type>
                <personalityDefName>Rimpsyche_SelfInterest</personalityDefName>
                <personalityMultipliers>
                    <li>
                        <personalityRange>-1.0~-0.5</personalityRange>
                        <multiplier>0.8</multiplier>
                        <flat>-2</flat>
                    </li>
                    <li>
                        <personalityRange>-0.49~0.0</personalityRange>
                        <multiplier>0.9</multiplier>
                    </li>
                    <li>
                        <personalityRange>0.01~0.5</personalityRange>
                        <multiplier>1.1</multiplier>
                    </li>
                    <li>
                        <personalityRange>0.51~1.0</personalityRange>
                        <multiplier>1.25</multiplier>
                        <flat>2</flat>
                    </li>
                </personalityMultipliers>
            </li>
            <li>
                <type>personalityOffset</type>
                <personalityDefName>Rimpsyche_Diligence</personalityDefName>
                <personalityMultipliers>
                    <li>
                        <personalityRange>-1.0~-0.5</personalityRange>
                        <multiplier>0.75</multiplier>
                        <flat>-2</flat>
                    </li>
                    <li>
                        <personalityRange>-0.49~0.0</personalityRange>
                        <multiplier>0.9</multiplier>
                    </li>
                    <li>
                        <personalityRange>0.01~0.5</personalityRange>
                        <multiplier>1.1</multiplier>
                    </li>
                    <li>
                        <personalityRange>0.51~1.0</personalityRange>
                        <multiplier>1.25</multiplier>
                        <flat>2</flat>
                    </li>
                </personalityMultipliers>
            </li>
        </conditions>

        <priorityRanges>
            <li>
                <validRange>0.01~0.14</validRange>
                <priority>10~15</priority>
                <description>I'm starving and need a meal fast</description>
            </li>
            <li>
                <validRange>0.15~0.29</validRange>
                <priority>7~10</priority>
                <description>I'm very hungry</description>
            </li>
            <li>
                <validRange>0.3~0.49</validRange>
                <priority>3~7</priority>
                <description>Starting to feel hungry</description>
            </li>
            <li>
                <validRange>0.5~0.79</validRange>
                <priority>0~0</priority>
                <description>I am not hungry for now</description>
            </li>
            <li>
                <validRange>0.8~1.0</validRange>
                <priority>0~0</priority>
                <description>I'm full for now</description>
            </li>
        </priorityRanges>

        <targetWorkGivers>
            <li>DoBillsCook</li>
            <li>DoBillsCookCampfire</li>
            <li>DoBillsButcherFlesh</li>
        </targetWorkGivers>

    </Autonomy.PriorityGiverDef>

    <!-- Cooking priority: feed hungry colony -->
    <Autonomy.PriorityGiverDef>
        <defName>CookColonyHunger</defName>
        <label>Cook by colony hunger</label>
        <description>Prioritizes cooking based on the colony's overall hunger level.</description>
        <isUrgent>true</isUrgent>

        <conditions>
            <li>
                <type>infoGiver</type>
                <infoDefName>ColonistFoodNeed</infoDefName>
                <requestLocalizedData>false</requestLocalizedData>
                <requestIndividualData>false</requestIndividualData>
                <requestNormalizedDistance>false</requestNormalizedDistance>
            </li>
            <li>
                <type>personalityOffset</type>
                <personalityDefName>Rimpsyche_Compassion</personalityDefName>
                <personalityMultipliers>
                    <li>
                        <personalityRange>-1.0~-0.5</personalityRange>
                        <multiplier>0.7</multiplier>
                        <flat>-3</flat>
                    </li>
                    <li>
                        <personalityRange>-0.49~0.0</personalityRange>
                        <multiplier>0.85</multiplier>
                    </li>
                    <li>
                        <personalityRange>0.01~0.5</personalityRange>
                        <multiplier>1.15</multiplier>
                    </li>
                    <li>
                        <personalityRange>0.51~1.0</personalityRange>
                        <multiplier>1.3</multiplier>
                        <flat>3</flat>
                    </li>
                </personalityMultipliers>
            </li>
            <li>
                <type>personalityOffset</type>
                <personalityDefName>Rimpsyche_Organization</personalityDefName>
                <personalityMultipliers>
                    <li>
                        <personalityRange>-1.0~-0.5</personalityRange>
                        <multiplier>0.8</multiplier>
                    </li>
                    <li>
                        <personalityRange>-0.49~0.0</personalityRange>
                        <multiplier>0.9</multiplier>
                    </li>
                    <li>
                        <personalityRange>0.01~0.5</personalityRange>
                        <multiplier>1.1</multiplier>
                    </li>
                    <li>
                        <personalityRange>0.51~1.0</personalityRange>
                        <multiplier>1.25</multiplier>
                        <flat>2</flat>
                    </li>
                </personalityMultipliers>
            </li>
        </conditions>

        <priorityRanges>
            <li>
                <validRange>0~0</validRange>
                <priority>0</priority>
                <description>No colonists needing meals</description>
            </li>
            <li>
                <validRange>0.01~0.14</validRange>
                <priority>10~15</priority>
                <description>Everyone's starving</description>
            </li>
            <li>
                <validRange>0.15~0.29</validRange>
                <priority>4~10</priority>
                <description>The colony is very hungry</description>
            </li>
            <li>
                <validRange>0.3~0.49</validRange>
                <priority>2~4</priority>
                <description>People are getting hungry</description>
            </li>
            <li>
                <validRange>0.5~0.79</validRange>
                <priority>0~0</priority>
                <description>People are well fed for now</description>
            </li>
            <li>
                <validRange>0.8~1.0</validRange>
                <priority>0~0</priority>
                <description>People are well fed for now</description>
            </li>
        </priorityRanges>

        <targetWorkGivers>
            <li>DoBillsCook</li>
            <li>DoBillsCookCampfire</li>
            <li>DoBillsButcherFlesh</li>
        </targetWorkGivers>

    </Autonomy.PriorityGiverDef>

