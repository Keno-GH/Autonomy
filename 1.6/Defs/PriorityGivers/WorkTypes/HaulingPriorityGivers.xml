<Defs>

    <!-- Prioritygiver for HaulGeneral, DeliverResourcesToFrames, DeliverResourcesToBlueprints,
    HaulMerge, RearmTurrets, Refuel, UnloadCarriers, HelpGatheringItemsForCaravan, 
    LoadTransporters, HaulToPortal, Strip based on the MovespeedInfoGiver, individualized,
    competitive, non-normalized, with a range from -999 to 999, effective range from -2.00
    to 3.00, with a priority range from -10 to 10, with -10 at -2.00 and 10 at 3.00, and 0
    at 0.00 -->

    <Autonomy.PriorityGiverDef>
        <defName>HaulMovespeedPriorityGiver</defName>
        <label>Hauling by movespeed</label>
        <description>Fast-footed pawns step up for hauling, while slower colonists hang back.</description>
        <isUrgent>false</isUrgent>

        <conditions>
            <li>
                <type>infoGiver</type>
                <infoDefName>MovespeedInfoGiver</infoDefName>
                <requestLocalizedData>false</requestLocalizedData>
                <requestIndividualData>true</requestIndividualData>
                <requestDistanceFromGlobal>true</requestDistanceFromGlobal>
            </li>
            <li>
                <type>personalityOffset</type>
                <personalityDefName>Rimpsyche_Diligence</personalityDefName>
                <personalityMultipliers>
                    <li>
                        <personalityRange>-1.0~-0.5</personalityRange>
                        <multiplier>0.75</multiplier>
                        <flat>-2</flat>
                    </li>
                    <li>
                        <personalityRange>-0.49~0.0</personalityRange>
                        <multiplier>0.9</multiplier>
                    </li>
                    <li>
                        <personalityRange>0.01~0.5</personalityRange>
                        <multiplier>1.1</multiplier>
                    </li>
                    <li>
                        <personalityRange>0.51~1.0</personalityRange>
                        <multiplier>1.25</multiplier>
                        <flat>2</flat>
                    </li>
                </personalityMultipliers>
            </li>
            <li>
                <type>personalityOffset</type>
                <personalityDefName>Rimpsyche_Tenacity</personalityDefName>
                <personalityMultipliers>
                    <li>
                        <personalityRange>-1.0~-0.5</personalityRange>
                        <multiplier>0.8</multiplier>
                        <flat>-1</flat>
                    </li>
                    <li>
                        <personalityRange>-0.49~0.0</personalityRange>
                        <multiplier>0.9</multiplier>
                    </li>
                    <li>
                        <personalityRange>0.01~0.5</personalityRange>
                        <multiplier>1.05</multiplier>
                    </li>
                    <li>
                        <personalityRange>0.51~1.0</personalityRange>
                        <multiplier>1.2</multiplier>
                        <flat>1</flat>
                    </li>
                </personalityMultipliers>
            </li>
            <li>
                <type>personalityOffset</type>
                <personalityDefName>Rimpsyche_Organization</personalityDefName>
                <personalityMultipliers>
                    <li>
                        <personalityRange>-1.0~-0.5</personalityRange>
                        <multiplier>0.8</multiplier>
                        <flat>-1</flat>
                    </li>
                    <li>
                        <personalityRange>-0.49~0.0</personalityRange>
                        <multiplier>0.9</multiplier>
                    </li>
                    <li>
                        <personalityRange>0.01~0.5</personalityRange>
                        <multiplier>1.1</multiplier>
                    </li>
                    <li>
                        <personalityRange>0.51~1.0</personalityRange>
                        <multiplier>1.25</multiplier>
                        <flat>2</flat>
                    </li>
                </personalityMultipliers>
            </li>
        </conditions>

        <priorityRanges>
            <li>
                <validRange>-999~-2</validRange>
                <priority>-10</priority>
                <description>I can barely drag my feet</description>
            </li>
            <li>
                <validRange>-1.99~-1</validRange>
                <priority>-8~-4</priority>
                <description>I'm too slow for this kind of stuff</description>
            </li>
            <li>
                <validRange>-0.99~-0.01</validRange>
                <priority>-4~0</priority>
                <description>There are faster people for hauling</description>
            </li>
            <li>
                <validRange>0</validRange>
                <priority>0</priority>
                <description>I can move like the rest so I can haul</description>
            </li>
            <li>
                <validRange>0.01~1.5</validRange>
                <priority>1~5</priority>
                <description>I am light and fast in comparison to the rest</description>
            </li>
            <li>
                <validRange>1.51~3</validRange>
                <priority>5~10</priority>
                <description>I am very fast in comparison to the rest</description>
            </li>
            <li>
                <validRange>3.01~999</validRange>
                <priority>10</priority>
                <description>I am amongst the fastest in the colony</description>
            </li>
        </priorityRanges>

        <targetWorkGivers>
            <li>HaulGeneral</li>
            <li>DeliverResourcesToFrames</li>
            <li>DeliverResourcesToBlueprints</li>
            <li>HaulMerge</li>
            <li>RearmTurrets</li>
            <li>Refuel</li>
            <li>UnloadCarriers</li>
            <li>HelpGatheringItemsForCaravan</li>
            <li>LoadTransporters</li>
            <li>HaulToPortal</li>
            <li>Strip</li>
        </targetWorkGivers>

    </Autonomy.PriorityGiverDef>

    <!-- Prioritygiver for HaulGeneral, using the OutsideDeteriorablesInfoGiver infogiver, 
    with a range from 0 to 9999, effective range from 0 to 1000, with a priority range
    from 0 to 20, with 0 at 0, 10 at 100, 15 at 500 and 20 at 1000 -->

    <Autonomy.PriorityGiverDef>
        <defName>HaulOutsideDeteriorablesPriorityGiver</defName>
        <label>Hauling by exposed deteriorables</label>
        <description>Boosts general hauling urgency when deteriorable stacks are left outside.</description>
        <isUrgent>false</isUrgent>

        <conditions>
            <li>
                <type>infoGiver</type>
                <infoDefName>OutsideDeteriorablesInfoGiver</infoDefName>
                <requestLocalizedData>false</requestLocalizedData>
                <requestIndividualData>false</requestIndividualData>
                <requestDistanceFromGlobal>false</requestDistanceFromGlobal>
            </li>
            <li>
                <type>personalityOffset</type>
                <personalityDefName>Rimpsyche_Organization</personalityDefName>
                <personalityMultipliers>
                    <li>
                        <personalityRange>-1.0~-0.5</personalityRange>
                        <multiplier>0.75</multiplier>
                        <flat>-2</flat>
                    </li>
                    <li>
                        <personalityRange>-0.49~0.0</personalityRange>
                        <multiplier>0.9</multiplier>
                    </li>
                    <li>
                        <personalityRange>0.01~0.5</personalityRange>
                        <multiplier>1.1</multiplier>
                    </li>
                    <li>
                        <personalityRange>0.51~1.0</personalityRange>
                        <multiplier>1.25</multiplier>
                        <flat>2</flat>
                    </li>
                </personalityMultipliers>
            </li>
            <li>
                <type>personalityOffset</type>
                <personalityDefName>Rimpsyche_Diligence</personalityDefName>
                <personalityMultipliers>
                    <li>
                        <personalityRange>-1.0~-0.5</personalityRange>
                        <multiplier>0.8</multiplier>
                        <flat>-1</flat>
                    </li>
                    <li>
                        <personalityRange>-0.49~0.0</personalityRange>
                        <multiplier>0.9</multiplier>
                    </li>
                    <li>
                        <personalityRange>0.01~0.5</personalityRange>
                        <multiplier>1.08</multiplier>
                    </li>
                    <li>
                        <personalityRange>0.51~1.0</personalityRange>
                        <multiplier>1.2</multiplier>
                        <flat>1</flat>
                    </li>
                </personalityMultipliers>
            </li>
            <li>
                <type>personalityOffset</type>
                <personalityDefName>Rimpsyche_Expectation</personalityDefName>
                <personalityMultipliers>
                    <li>
                        <personalityRange>-1.0~-0.5</personalityRange>
                        <multiplier>0.85</multiplier>
                        <flat>-1</flat>
                    </li>
                    <li>
                        <personalityRange>-0.49~0.0</personalityRange>
                        <multiplier>0.95</multiplier>
                    </li>
                    <li>
                        <personalityRange>0.01~0.5</personalityRange>
                        <multiplier>1.05</multiplier>
                    </li>
                    <li>
                        <personalityRange>0.51~1.0</personalityRange>
                        <multiplier>1.2</multiplier>
                        <flat>2</flat>
                    </li>
                </personalityMultipliers>
            </li>
        </conditions>

        <priorityRanges>
            <li>
                <validRange>0</validRange>
                <priority>0</priority>
                <description>Nothing is deteriorating outside right now</description>
            </li>
            <li>
                <validRange>1~50</validRange>
                <priority>2~7</priority>
                <description>Some stuff is deteriorating outside</description>
            </li>
            <li>
                <validRange>51~100</validRange>
                <priority>7~10</priority>
                <description>There are lots of items deteriorating outside</description>
            </li>
            <li>
                <validRange>101~300</validRange>
                <priority>10~13</priority>
                <description>Our supplies are spoiling outside</description>
            </li>
            <li>
                <validRange>301~500</validRange>
                <priority>13~15</priority>
                <description>Too many supplies rotting away, we need to move them inside</description>
            </li>
            <li>
                <validRange>501~800</validRange>
                <priority>15~18</priority>
                <description>Too many supplies decaying away, we need to move them inside</description>
            </li>
            <li>
                <validRange>801~1000</validRange>
                <priority>18~20</priority>
                <description>Too many supplies decaying away, we need to move them inside</description>
            </li>
            <li>
                <validRange>1001~9999</validRange>
                <priority>20</priority>
                <description>Too many supplies decaying away, we need to move them inside</description>
            </li>
        </priorityRanges>

        <targetWorkGivers>
            <li>HaulGeneral</li>
        </targetWorkGivers>

    </Autonomy.PriorityGiverDef>

</Defs>